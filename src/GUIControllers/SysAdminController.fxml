<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.AnchorPane?>
<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="GUIControllers.SysAdminController">
   <children>
      <TabPane prefHeight="400.0" prefWidth="600.0" tabClosingPolicy="UNAVAILABLE">
        <tabs>
          <Tab text="Create/Delete User">
            <content>
              <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                     <children>
                        <TextField fx:id="createUsername" layoutX="14.0" layoutY="14.0" promptText="Username" />
                        <TextField fx:id="createPassword" layoutX="14.0" layoutY="49.0" promptText="Password" />
                        <TextField fx:id="createFirstName" layoutX="14.0" layoutY="82.0" promptText="First Name" />
                        <TextField fx:id="createLastName" layoutX="14.0" layoutY="114.0" promptText="Last Name" />
                        <TextField fx:id="createEmail" layoutX="14.0" layoutY="146.0" promptText="Email" />
                        <TextField fx:id="createPhoneNumber" layoutX="14.0" layoutY="177.0" promptText="Phone Number" />
                        <ChoiceBox fx:id="createUserChoice" layoutX="173.0" layoutY="14.0" prefWidth="150.0" />
                        <TextField fx:id="createVanName" editable="false" layoutX="174.0" layoutY="49.0" promptText="Van Name" />
                        <Button fx:id="createButton" layoutX="50.0" layoutY="210.0" mnemonicParsing="false" onAction="#doCreateButton" text="Create User" />
                        <TextArea fx:id="createOutput" editable="false" layoutX="173.0" layoutY="83.0" prefHeight="239.0" prefWidth="239.0" />
                        <Button layoutX="544.0" layoutY="4.0" mnemonicParsing="false" onAction="#doLogoutButton" text="Logout" />
                        <TextField fx:id="deleteUsername" layoutX="422.0" layoutY="82.0" promptText="Username" />
                        <Button fx:id="deleteButton" layoutX="458.0" layoutY="114.0" mnemonicParsing="false" onAction="#doDeleteButton" text="Delete User" />
                     </children></AnchorPane>
            </content>
          </Tab>
        </tabs>
      </TabPane>
   </children>
</AnchorPane>
